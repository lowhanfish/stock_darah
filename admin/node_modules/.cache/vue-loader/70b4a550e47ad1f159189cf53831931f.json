{"remainingRequest":"/Users/simplephi/Documents/riswan/stock_darah/admin/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/simplephi/Documents/riswan/stock_darah/admin/src/views/auth/login.vue?vue&type=style&index=0&id=fcf1ae48&scoped=true&lang=css&","dependencies":[{"path":"/Users/simplephi/Documents/riswan/stock_darah/admin/src/views/auth/login.vue","mtime":1758932938820},{"path":"/Users/simplephi/Documents/riswan/stock_darah/admin/node_modules/css-loader/dist/cjs.js","mtime":1758930417656},{"path":"/Users/simplephi/Documents/riswan/stock_darah/admin/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":1758930412642},{"path":"/Users/simplephi/Documents/riswan/stock_darah/admin/node_modules/postcss-loader/src/index.js","mtime":1758930415422},{"path":"/Users/simplephi/Documents/riswan/stock_darah/admin/node_modules/vue-cli-plugin-quasar/lib/loader.auto-import.js","mtime":1758930412679},{"path":"/Users/simplephi/Documents/riswan/stock_darah/admin/node_modules/cache-loader/dist/cjs.js","mtime":1758930418112},{"path":"/Users/simplephi/Documents/riswan/stock_darah/admin/node_modules/vue-loader/lib/index.js","mtime":1758930412640}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["login.vue"],"names":[],"mappings":";AAgRA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"login.vue","sourceRoot":"src/views/auth","sourcesContent":["<template>\n  <div class=\"blood-login-bg\">\n    <!-- Background Pattern -->\n    <div class=\"blood-pattern\"></div>\n    \n    <!-- Main Container -->\n    <div class=\"row items-center min-height-inherit\">\n      <div class=\"col-12 col-md-6 col-lg-4 offset-md-3 offset-lg-4\">\n        \n        <!-- Login Card -->\n        <div class=\"blood-login-card shadow-10\">\n          \n          <!-- Header Section -->\n          <div class=\"login-header text-center\">\n            <div class=\"blood-drop-icon\">\n              <q-icon name=\"water_drop\" size=\"48px\" color=\"red-6\" />\n            </div>\n            <h4 class=\"blood-title text-red-8 q-mb-xs\">Blood Stock System</h4>\n            <p class=\"blood-subtitle text-grey-7 q-mb-lg\">Sistem Manajemen Stok Darah Rumah Sakit</p>\n            \n            <!-- Hospital Logo/Image -->\n            <div class=\"hospital-logo q-mb-md\">\n              <q-avatar size=\"80px\" color=\"red-1\" text-color=\"red-8\">\n                <q-icon name=\"local_hospital\" size=\"40px\" />\n              </q-avatar>\n            </div>\n          </div>\n\n          <q-separator class=\"q-mb-lg\" color=\"red-2\" />\n\n          <!-- Alert Section -->\n          <div v-if=\"errorMessage\" class=\"q-mb-md\">\n            <q-banner class=\"blood-alert text-white\" rounded>\n              <template v-slot:avatar>\n                <q-icon name=\"warning\" color=\"white\" />\n              </template>\n              <strong>Peringatan!</strong> {{ errorMessage }}\n            </q-banner>\n          </div>\n\n          <!-- Login Form -->\n          <form @submit.prevent=\"btn_login()\" class=\"login-form\">\n            \n            <!-- Username Field -->\n            <div class=\"form-group q-mb-md\">\n              <label class=\"field-label text-red-8 text-weight-medium q-mb-xs\">Username</label>\n              <q-input \n                v-model=\"user.username\" \n                outlined \n                :dense=\"false\"\n                class=\"blood-input\" \n                placeholder=\"Masukkan username Anda\"\n                bg-color=\"white\"\n                color=\"red-6\"\n              >\n                <template v-slot:prepend>\n                  <q-icon name=\"person\" color=\"red-6\" />\n                </template>\n              </q-input>\n            </div>\n\n            <!-- Password Field -->\n            <div class=\"form-group q-mb-xl\">\n              <label class=\"field-label text-red-8 text-weight-medium q-mb-xs\">Password</label>\n              <q-input \n                v-model=\"user.password\" \n                outlined \n                :dense=\"false\"\n                class=\"blood-input\" \n                placeholder=\"Masukkan password Anda\"\n                :type=\"showPassword ? 'text' : 'password'\"\n                bg-color=\"white\"\n                color=\"red-6\"\n              >\n                <template v-slot:prepend>\n                  <q-icon name=\"lock\" color=\"red-6\" />\n                </template>\n                <template v-slot:append>\n                  <q-icon \n                    :name=\"showPassword ? 'visibility' : 'visibility_off'\"\n                    color=\"red-6\"\n                    class=\"cursor-pointer\"\n                    @click=\"showPassword = !showPassword\"\n                  />\n                </template>\n              </q-input>\n            </div>\n\n            <!-- Login Button -->\n            <q-btn \n              type=\"submit\" \n              unelevated \n              size=\"lg\"\n              class=\"full-width blood-login-btn text-weight-bold\"\n              icon-right=\"login\"\n              label=\"MASUK SISTEM\"\n              :loading=\"isLoading\"\n              loading-text=\"Memverifikasi...\"\n            />\n            \n          </form>\n\n          <!-- Additional Options -->\n          <div class=\"text-center q-mt-md\">\n            <q-btn \n              flat \n              no-caps \n              color=\"red-6\" \n              class=\"text-caption\"\n              label=\"Lupa Password?\"\n            />\n          </div>\n\n          <q-separator class=\"q-my-lg\" color=\"red-2\" />\n\n          <!-- Footer -->\n          <div class=\"login-footer text-center\">\n            <div class=\"blood-stats row q-mb-md\">\n              <div class=\"col-4 text-center\">\n                <div class=\"stat-number text-red-6 text-weight-bold\">A+</div>\n                <div class=\"stat-label text-grey-6 text-caption\">Ready</div>\n              </div>\n              <div class=\"col-4 text-center\">\n                <div class=\"stat-number text-red-6 text-weight-bold\">B+</div>\n                <div class=\"stat-label text-grey-6 text-caption\">Ready</div>\n              </div>\n              <div class=\"col-4 text-center\">\n                <div class=\"stat-number text-red-6 text-weight-bold\">O+</div>\n                <div class=\"stat-label text-grey-6 text-caption\">Ready</div>\n              </div>\n            </div>\n            \n            <p class=\"copyright-text text-grey-5 text-caption q-mb-none\">\n              Â© 2024 Blood Stock Management System\n              <br>\n              Rumah Sakit Umum Daerah\n            </p>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- Floating Blood Drops Animation -->\n    <div class=\"floating-drops\">\n      <div class=\"drop drop-1\"></div>\n      <div class=\"drop drop-2\"></div>\n      <div class=\"drop drop-3\"></div>\n      <div class=\"drop drop-4\"></div>\n      <div class=\"drop drop-5\"></div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Joi from \"joi\";\n\nconst schema = Joi.object().keys({\n  username: Joi.string().regex(/^[a-zA-Z0-9_]*$/).min(3).max(13).required(),\n  password: Joi.string().min(6).required(),\n});\n\nimport { Notify } from 'quasar'\n\nexport default {\n  name: \"blood_stock_login\",\n  data: function () {\n    return {\n      errorMessage: '',\n      showPassword: false,\n      isLoading: false,\n      user: {\n        username: \"\",\n        password: \"\"\n      },\n      url: {\n        LOGIN_URL: this.$store.state.url.URL_APP + \"auth/login\",\n      }\n    }\n  },\n  watch: {\n    user: {\n      handler() {\n        this.errorMessage = \"\";\n      },\n      deep: true\n    }\n  },\n  methods: {\n    btn_login: function () {\n      this.errorMessage = '';\n      \n      if (this.validUser()) {\n        this.isLoading = true;\n        this.$store.commit(\"shoWLoading\");\n\n        const body = {\n          username: this.user.username,\n          password: this.user.password\n        }\n\n        fetch(this.url.LOGIN_URL, {\n          method: 'POST',\n          headers: {\n            'content-type': 'application/json',\n          },\n          body: JSON.stringify(body),\n        })\n        .then((response) => {\n          if (response.ok) {\n            return response.json();\n          }\n          return response.json().then(error => {\n            throw new Error(error.message);\n          });\n        })\n        .then((result) => {\n          // Menyimpan token dan profile\n          localStorage.token = result.token;\n          localStorage.profile = JSON.stringify(result.profile);\n          \n          const profile = result.profile;\n          let tipe = null;\n          \n          if (profile.db_csrkonsel) {\n            tipe = profile.db_csrkonsel;\n          } else if (profile.profile && profile.profile.db_csrkonsel) {\n            tipe = profile.profile.db_csrkonsel;\n          }\n\n          localStorage.tipe = tipe;\n\n          setTimeout(() => {\n            this.isLoading = false;\n            this.$store.commit(\"hideLoading\");\n            \n            // Routing berdasarkan tipe user untuk sistem darah\n            if (tipe === \"1\" || tipe === \"5\") {\n              this.$router.push(\"/dashboard-admin\"); // Admin/Doctor\n            } else if (tipe === \"4\") {\n              this.$router.push(\"/dashboard-lab\"); // Lab Technician\n            } else {\n              this.$router.push(\"/dashboard-staff\"); // Staff\n            }\n          }, 1500);\n        })\n        .catch(error => {\n          setTimeout(() => {\n            this.isLoading = false;\n            this.$store.commit(\"hideLoading\");\n            this.errorMessage = error.message;\n          }, 1000);\n        });\n      }\n    },\n    \n    validUser: function () {\n      const result = Joi.validate(this.user, schema);\n      if (result.error === null) {\n        return true;\n      }\n      if (result.error.message.includes(\"username\")) {\n        this.errorMessage = \"Format username tidak valid (3-13 karakter, alfanumerik)\";\n      } else {\n        this.errorMessage = \"Password minimal 6 karakter\";\n      }\n      return false;\n    }\n  }\n};\n</script>\n\n<style scoped>\n/* Reset dan Base Styles */\n* {\n  box-sizing: border-box;\n}\n\n/* Background Styles */\n.blood-login-bg {\n  min-height: 100vh!important;\n  width: 100vw;\n  background: linear-gradient(135deg, #ffebee 0%, #fce4ec 25%, #f8bbd9 50%, #f48fb1 75%, #f06292 100%);\n  position: fixed;\n  top: 0;\n  left: 0;\n  overflow: hidden;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  padding: 20px;\n}\n\n.blood-pattern {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  opacity: 0.08;\n  background-image: \n    radial-gradient(circle at 15% 25%, #e53935 3px, transparent 3px),\n    radial-gradient(circle at 85% 35%, #d32f2f 2px, transparent 2px),\n    radial-gradient(circle at 45% 75%, #c62828 2.5px, transparent 2.5px),\n    radial-gradient(circle at 75% 65%, #b71c1c 1.5px, transparent 1.5px),\n    radial-gradient(circle at 25% 85%, #e53935 2px, transparent 2px);\n  background-size: 120px 120px, 90px 90px, 140px 140px, 80px 80px, 110px 110px;\n  animation: pattern-float 20s ease-in-out infinite;\n}\n\n/* Login Card */\n.blood-login-card {\n  background: rgba(255, 255, 255, 0.97);\n  backdrop-filter: blur(20px);\n  -webkit-backdrop-filter: blur(20px);\n  border-radius: 32px;\n  padding: 48px 40px;\n  border: 2px solid rgba(229, 57, 53, 0.08);\n  max-width: 520px;\n  width: 100%;\n  margin: 0 auto!important;\n  position: relative;\n  z-index: 10;\n  box-shadow: \n    0 20px 60px rgba(229, 57, 53, 0.15),\n    0 8px 32px rgba(0, 0, 0, 0.05),\n    inset 0 1px 0 rgba(255, 255, 255, 0.8);\n  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);\n}\n\n.blood-login-card:hover {\n  transform: translateY(-4px);\n  box-shadow: \n    0 32px 80px rgba(229, 57, 53, 0.2),\n    0 12px 40px rgba(0, 0, 0, 0.08),\n    inset 0 1px 0 rgba(255, 255, 255, 0.9);\n}\n\n/* Header Styles */\n.login-header {\n  margin-bottom: 32px;\n  text-align: center;\n}\n\n.blood-drop-icon {\n  margin-bottom: 20px;\n  animation: pulse-gentle 3s ease-in-out infinite;\n  filter: drop-shadow(0 4px 8px rgba(229, 57, 53, 0.3));\n}\n\n.blood-title {\n  font-size: 32px;\n  font-weight: 800;\n  margin: 0;\n  letter-spacing: -0.8px;\n  background: linear-gradient(135deg, #d32f2f 0%, #e53935 50%, #f44336 100%);\n  -webkit-background-clip: text;\n  -webkit-text-fill-color: transparent;\n  background-clip: text;\n  text-shadow: 0 2px 4px rgba(229, 57, 53, 0.1);\n}\n\n.blood-subtitle {\n  font-size: 15px;\n  margin: 12px 0 0 0;\n  line-height: 1.5;\n  color: #666;\n  font-weight: 500;\n}\n\n.hospital-logo {\n  animation: float-smooth 4s ease-in-out infinite;\n  filter: drop-shadow(0 4px 12px rgba(229, 57, 53, 0.2));\n}\n\n/* Form Styles */\n.login-form {\n  margin: 32px 0;\n}\n\n.form-group {\n  margin-bottom: 24px;\n  position: relative;\n}\n\n.field-label {\n  display: block;\n  font-size: 15px;\n  font-weight: 600;\n  margin-bottom: 8px;\n  color: #d32f2f;\n  letter-spacing: 0.2px;\n}\n\n.blood-input {\n  border-radius: 16px;\n  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);\n}\n\n.blood-input .q-field__control {\n  border-radius: 16px !important;\n  height: 64px;\n  font-size: 16px;\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.04);\n  transition: all 0.3s ease;\n}\n\n.blood-input .q-field__control::before {\n  border-color: rgba(229, 57, 53, 0.15) !important;\n  border-width: 2px !important;\n}\n\n.blood-input .q-field__control:hover::before {\n  border-color: rgba(229, 57, 53, 0.3) !important;\n}\n\n.blood-input.q-field--focused .q-field__control::before {\n  border-color: #e53935 !important;\n  box-shadow: 0 0 0 3px rgba(229, 57, 53, 0.1) !important;\n}\n\n.blood-input .q-field__native {\n  padding-left: 16px !important;\n  font-weight: 500;\n}\n\n.blood-input .q-icon {\n  font-size: 24px;\n  margin-right: 4px;\n}\n\n/* Login Button */\n.blood-login-btn {\n  background: linear-gradient(135deg, #e53935 0%, #d32f2f 50%, #c62828 100%);\n  color: white;\n  border-radius: 20px;\n  height: 64px;\n  font-size: 17px;\n  font-weight: 700;\n  letter-spacing: 1px;\n  text-transform: uppercase;\n  transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);\n  box-shadow: \n    0 8px 32px rgba(229, 57, 53, 0.4),\n    0 4px 16px rgba(0, 0, 0, 0.1);\n  position: relative;\n  overflow: hidden;\n}\n\n.blood-login-btn::before {\n  content: '';\n  position: absolute;\n  top: 0;\n  left: -100%;\n  width: 100%;\n  height: 100%;\n  background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.2), transparent);\n  transition: left 0.6s ease;\n}\n\n.blood-login-btn:hover {\n  transform: translateY(-3px);\n  box-shadow: \n    0 12px 40px rgba(229, 57, 53, 0.5),\n    0 6px 20px rgba(0, 0, 0, 0.15);\n}\n\n.blood-login-btn:hover::before {\n  left: 100%;\n}\n\n.blood-login-btn:active {\n  transform: translateY(-1px);\n  transition: all 0.1s ease;\n}\n\n/* Alert Styles */\n.blood-alert {\n  background: linear-gradient(135deg, #e53935, #d32f2f);\n  border-radius: 16px;\n  animation: slideInDown 0.6s cubic-bezier(0.4, 0, 0.2, 1);\n  box-shadow: 0 8px 24px rgba(229, 57, 53, 0.3);\n  border: 1px solid rgba(255, 255, 255, 0.2);\n}\n\n/* Stats Section */\n.blood-stats {\n  background: linear-gradient(135deg, rgba(229, 57, 53, 0.03), rgba(229, 57, 53, 0.08));\n  border-radius: 20px;\n  padding: 20px 16px;\n  margin: 20px 0;\n  border: 1px solid rgba(229, 57, 53, 0.1);\n  backdrop-filter: blur(10px);\n}\n\n.stat-number {\n  font-size: 20px;\n  font-weight: 800;\n  color: #d32f2f;\n  text-shadow: 0 1px 2px rgba(229, 57, 53, 0.1);\n}\n\n.stat-label {\n  font-size: 11px;\n  text-transform: uppercase;\n  letter-spacing: 1px;\n  font-weight: 600;\n  color: #666;\n  margin-top: 4px;\n}\n\n/* Footer */\n.login-footer {\n  margin-top: 32px;\n}\n\n.copyright-text {\n  font-size: 13px;\n  line-height: 1.5;\n  color: #888;\n  font-weight: 500;\n}\n\n/* Floating Animation */\n.floating-drops {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  pointer-events: none;\n  z-index: 1;\n}\n\n.drop {\n  position: absolute;\n  width: 16px;\n  height: 16px;\n  background: linear-gradient(135deg, rgba(229, 57, 53, 0.4), rgba(211, 47, 47, 0.6));\n  border-radius: 50% 50% 50% 0;\n  transform: rotate(-45deg);\n  animation: float-drop 8s infinite ease-in-out;\n  filter: blur(0.5px);\n}\n\n.drop-1 {\n  top: 15%;\n  left: 8%;\n  animation-delay: 0s;\n  width: 12px;\n  height: 12px;\n}\n\n.drop-2 {\n  top: 65%;\n  right: 12%;\n  animation-delay: 2.5s;\n  width: 20px;\n  height: 20px;\n}\n\n.drop-3 {\n  bottom: 25%;\n  left: 15%;\n  animation-delay: 5s;\n  width: 14px;\n  height: 14px;\n}\n\n/* Additional floating elements */\n.drop-4 {\n  top: 40%;\n  right: 25%;\n  animation-delay: 1s;\n  width: 10px;\n  height: 10px;\n}\n\n.drop-5 {\n  bottom: 50%;\n  left: 80%;\n  animation-delay: 6s;\n  width: 18px;\n  height: 18px;\n}\n\n/* Separator Styles */\n.q-separator {\n  opacity: 0.6;\n}\n\n/* Button Additional Styles */\n.q-btn--flat.text-caption {\n  font-weight: 600;\n  letter-spacing: 0.3px;\n  transition: all 0.3s ease;\n  border-radius: 8px;\n  padding: 8px 16px;\n}\n\n.q-btn--flat.text-caption:hover {\n  background-color: rgba(229, 57, 53, 0.08);\n  transform: translateY(-1px);\n}\n\n/* Animations */\n@keyframes pulse-gentle {\n  0%, 100% { \n    transform: scale(1); \n    filter: drop-